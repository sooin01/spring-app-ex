<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tbUser">
	
	<select id="retrieveUsers" resultType="com.my.app.app1.vo.TbUserVo">
		select user_id
		     , user_name
		     , create_dt
		     , update_dt
         from public.tb_user
	</select>

</mapper>