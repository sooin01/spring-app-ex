<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tbUser">
	
	<select id="retrieveUsers" resultType="com.my.app.app1.vo.TbUserVo">
		SELECT "USER_ID"
		     , "USER_NAME"
		     , "CREATE_DT"
		     , "UPDATE_DT"
         FROM public."TB_USER"
	</select>

</mapper>